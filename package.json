{
  "name": "ac",
  "version": "1.0.0",
  "type": "module",
  "description": "AI Context CLI - 统一管理 AI 编程上下文与提示词的命令行工具",
  "keywords": [
    "oclif"
  ],
  "homepage": "https://github.com/astro-tpl/ac",
  "bugs": "https://github.com/astro-tpl/ac/issues",
  "repository": "astro-tpl/ac",
  "license": "MIT",
  "author": "AstroTpl",
  "exports": "./dist/index.js",
  "bin": {
    "ac": "./bin/run.js"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^3.15.1",
    "@oclif/plugin-help": "^6.0.8",
    "@oclif/plugin-update": "^4.7.4",
    "@testing-library/react": "^16.3.0",
    "clipboardy": "^4.0.0",
    "fuzzysort": "^2.0.4",
    "ink": "^4.4.1",
    "ink-text-input": "^5.0.1",
    "js-yaml": "^4.1.0",
    "pinyin-pro": "^3.27.0",
    "react": "^18.2.0",
    "simple-git": "^3.20.0",
    "table": "^6.9.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3.1.3",
    "@types/js-yaml": "^4.0.8",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^18",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.3.7",
    "eslint": "^8.55.0",
    "eslint-config-oclif": "^5.0.0",
    "eslint-config-oclif-typescript": "^3.0.8",
    "jsdom": "^26.1.0",
    "oclif": "^4.1.0",
    "prettier": "^3.1.1",
    "react-dom": "^18.3.1",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "oclif": {
    "bin": "ac",
    "dirname": "ac",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-update"
    ],
    "topicSeparator": " ",
    "topics": {
      "repo": {
        "description": "管理模板仓库"
      }
    }
  },
  "scripts": {
    "build": "tsc -b && tsc-alias && node fix-esm-imports.js",
    "build:manifest": "oclif manifest",
    "lint": "eslint . --ext .ts",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "vitest --run",
    "version": "oclif readme && git add README.md",
    "dev": "tsx src/index.ts",
    "clean": "rm -rf dist",
    "pack": "oclif pack tarballs",
    "upload": "oclif upload tarballs"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "types": "dist/index.d.ts"
}
